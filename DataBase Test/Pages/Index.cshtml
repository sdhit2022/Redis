@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="text-center">
    <h1 class="display-4">Result</h1>
</div>

<table id="dataTable_1" class="table table-hover table-bordered cell-border row-border" style="width:100%">
                            <thead>
                                <tr>
                                    <th>DB</th>
                                    <th>Insert</th>
                                    <th>Update</th>
                                    <th>Select</th>
                                    <th>Delete</th>
                                </tr>
                            </thead>
                            <tbody>
                              
                                @for(int i=0;i<@Model.Tables.Count;i++){
                                      <tr>
                                     <td>@Model.Tables[i].DBName</td>
                                     <td>@Model.Tables[i].Insert</td>
                                     <td>@Model.Tables[i].Update</td>
                                     <td>@Model.Tables[i].Select</td>
                                     <td>@Model.Tables[i].Delete</td>
                                     </tr>
                                }
                                
                            </tbody>
 </table>


